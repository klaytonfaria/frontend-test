    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "shell-arguments": "^1.0.4"
  },
  "devDependencies": {
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "^6.2.10",
